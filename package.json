{
  "name": "mobiledope",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules",
    "lint:cpd": "jscpd . --config .jscpd.json",
    "lint:cpd:ci": "jscpd . --config .jscpd.json --threshold 5",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level=high",
    "prepare": "husky"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^7.8.5",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "date-fns": "^4.1.0",
    "expo": "~54.0.30",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "~19.0.18",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-keep-awake": "~15.0.8",
    "expo-location": "~19.0.8",
    "expo-print": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-sharing": "~14.0.8",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-hook-form": "^7.69.0",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-view-shot": "4.0.3",
    "victory-native": "^41.20.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "jscpd": "^4.0.8",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.5",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "sh -c 'ESLINT_USE_FLAT_CONFIG=false eslint --fix \"$@\"' --",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.md": [
      "markdownlint"
    ]
  },
  "private": true
}
